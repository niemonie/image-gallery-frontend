{
  "name": "ImageGallery",
  "version": "1.0.0",
  "dependencies": {
    "@types/styled-components": "^5.0.1",
    "and": "^0.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "husky": "^4.2.3",
    "prettier": "2.0.0",
    "pretty-quick": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "BROWSER=none PORT=3003 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": [
        "error",
        {
          "fixToUnknown": true,
          "ignoreRestArgs": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
